package com.job.assignment1;
import java.util.*;
public class rectanglepattern 
{
	public static void main(String[] args) {
		int n,m,i,j;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		m=sc.nextInt();
		String c=sc.next();
		for(i=0;i<n;i++)
		{
			for(j=0;j<m;j++)
			{
				System.out.print(c);
			}
			System.out.println();
		}
	}
}
