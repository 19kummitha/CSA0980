package com.job.assignment1;
import java.util.*;
public class pattern122334444333 
{
	public static void main(String[] args)
	{
		int n,i,j;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		for(i=1;i<n;i++)
		{
			for(j=1;j<n;j++)
			{
				if(i>=j)
				{
					System.out.print(" "+i);
				}
			}
			System.out.println();
		}
		for(i=n-2;i>=1;--i)
		{
			for(j=0;j<i;++j)
			{
				System.out.print(" "+i);
			}
			System.out.println();
		}
	}
}
