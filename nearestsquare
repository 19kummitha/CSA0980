package com.job.assignment1;
import java.util.*;
public class squareroot 
{
	public static int sqrt(int x)
	{
		if(x==1 || x==0)
		{
			return x;
		}
		int i=1,res=1;
		while(res<=x)
		{
			i++;
			res=i*i;
		}
		return i-1;
	}
	public static void main(String[] args) {
		int n;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		System.out.println(sqrt(n));
	}
}
